{
  "name": "swiggy-tests",
  "version": "1.0.0",
  "description": "Swiggy tests project using version 5 WebdriverIO & TypeScript.",
  "repository": "https://github.com/s3anand21/swiggy-tests",
  "scripts": {
    "test": "./node_modules/.bin/wdio wdio.conf.js",
    "lint": "tslint --fix 'test/**/*.ts' && tslint --fix 'src/**/*.ts' && tslint --fix 'util/**/*.ts'",
    "selenium:install": "npx selenium-standalone install",
    "selenium:start": "npx selenium-standalone start --",
    "allure-report": "allure generate target/allure-results --clean -o target/allure-report && allure open target/allure-report",
    "junit-report": "node_modules/.bin/junit-viewer --results=./target/junit-results --save=junit-results.html && open junit-results.html"
  },
  "author": "Anand Ganesh [s3anand21@gmail.com]",
  "license": "ISC",
  "keywords": [
    "swiggy",
    "typescript",
    "webdriverio",
    "mocha",
    "allure"
  ],
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/del": "^3.0.1",
    "@types/gulp": "^4.0.5",
    "@types/minimist": "^1.2.0",
    "@types/mocha": "^5.2.5",
    "@types/node": "^10.12.20",
    "@wdio/allure-reporter": "^5.6.0",
    "@wdio/cli": "^5.6.0",
    "@wdio/dot-reporter": "^5.6.0",
    "@wdio/junit-reporter": "^5.6.0",
    "@wdio/local-runner": "^5.6.0",
    "@wdio/mocha-framework": "^5.6.0",
    "@wdio/selenium-standalone-service": "^5.6.0",
    "@wdio/spec-reporter": "^5.6.0",
    "@wdio/sync": "^5.6.0",
    "allure-commandline": "^2.9.0",
    "chai": "^4.2.0",
    "del": "^3.0.0",
    "gulp": "^4.0.0",
    "gulp-mocha": "^6.0.0",
    "gulp-run-command": "^0.0.9",
    "junit-viewer": "^4.11.1",
    "minimist": "^1.2.0",
    "mocha": "^6.0.1",
    "mocha-allure-reporter": "^1.4.0",
    "mocha-junit-reporter": "^1.18.0",
    "mocha-multi-reporters": "^1.1.7",
    "selenium-standalone": "^6.15.4",
    "ts-node": "^8.0.2",
    "tsconfig-paths": "^3.8.0",
    "tslint": "^5.12.1",
    "tslint-config-airbnb": "^5.11.1",
    "typescript": "^3.3.3333",
    "webdriverio": "^5.6.0"
  }
}
